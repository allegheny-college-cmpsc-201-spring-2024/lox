// Uncomment import to test import functionality
import "fish";

class fishingTrip {

  init() {
    this.count = 0;
  }

  goFish() {
    var chance = random(1, 10);
    if (chance == 10) {
      this.count = this.count + 1;
      this.fish = fish();
    }
  }

  caught {
    if (this.count > 0) {
      return this.fish.type;
    }
  }

}

var trip = fishingTrip();
for(;;) {
  trip.goFish();
  if (trip.count > 0) {
    break;
  }
}

print "It's a " + trip.caught + "!";
